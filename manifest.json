{
  "manifest_version": 2,

  "name": "FourChrome",
  "description": "Adpearance Chrome extension for Google Ads",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "script.js"
      ]
    }
  ],
   "web_accessible_resources": [
    "images/*.png"
  ]
}
